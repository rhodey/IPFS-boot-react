{
  "name": "IPFS-boot-react",
  "version": "1.0.0",
  "description": "IPFS-boot react.js starter",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/rhodey/IPFS-boot-react.git"
  },
  "homepage": "https://lock.host",
  "scripts": {
    "lint": "eslint .",
    "move": "mkdir -p dist/_static && find dist/ -mindepth 1 -maxdepth 1 -not -name '_static' -not -name 'index.html' -exec mv {} dist/_static \\;",
    "build": "rm -rf dist/ && vite build --base=/_static/ && npm run move",
    "dev": "vite --port 8080"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.3"
  },
  "keywords": [
    "ipfs",
    "update",
    "bootloader",
    "hash",
    "p2p",
    "react"
  ]
}
